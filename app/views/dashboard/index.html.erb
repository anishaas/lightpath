<h1> Student Dashboard </h1><br>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Most Popular Events</h3>
  </div>
  <div class="panel-body">
      <!-- Table -->
   <table class="table">
     <thead>
       <th>Course</th>
       <th>Teacher</th>
       <th colspan="2"></th>
     </thead>

     <tbody>
       <% Course.each do |course| %>
        <tr>
          <td><%= course.name %></td>
          <td><%= course.teacher %></td>
        </tr>
       <% end %>
     </tbody>

   </table>

  </div>
</div>
